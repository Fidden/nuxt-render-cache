{
  "name": "nuxt-render-cache",
  "version": "0.1.0",
  "description": "Powerful caching library for Nuxt 3 applications. Solves SSR performance issues with flexible caching system supporting TTL, locks, and distributed servers. Cache Vue components and async data with soft/hard TTL strategy.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Fidden/nuxt-cache.git"
  },
  "license": "MIT",
  "keywords": [
    "nuxt3",
    "cache",
    "caching",
    "vue",
    "ssr",
    "performance",
    "ttl",
    "redis",
    "memory",
    "storage",
    "component-cache",
    "data-cache",
    "server-rendering"
  ],
  "author": "Alexandr Lisenkov <fiddenhook@gmail.com>",
  "homepage": "https://github.com/Fidden/nuxt-cache#readme",
  "bugs": {
    "url": "https://github.com/Fidden/nuxt-cache/issues"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "NODE_OPTIONS='--max-old-space-size=8096' nuxt-module-build build",
    "dev": "npm run dev:prepare && nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/kit": "^4.1.1",
    "ioredis": "^5.7.0"
  },
  "devDependencies": {
    "@nuxt/devtools": "^2.6.3",
    "@nuxt/eslint-config": "^1.9.0",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^4.1.1",
    "@nuxt/test-utils": "^3.19.2",
    "@types/node": "latest",
    "changelogen": "^0.6.2",
    "eslint": "^9.34.0",
    "nuxt": "^4.1.1",
    "typescript": "~5.9.2",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.6"
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "printWidth": 80
  }
}
